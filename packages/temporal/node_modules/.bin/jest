#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxim/repos/n8n/node_modules/.pnpm/jest@29.7.0_@types+node@20.19.21_ts-node@10.9.2_@swc+core@1.15.7_@swc+helpers@0.5.17__@_897206e04065c0608fc74f2b315f766b/node_modules/jest/bin/node_modules:/Users/maxim/repos/n8n/node_modules/.pnpm/jest@29.7.0_@types+node@20.19.21_ts-node@10.9.2_@swc+core@1.15.7_@swc+helpers@0.5.17__@_897206e04065c0608fc74f2b315f766b/node_modules/jest/node_modules:/Users/maxim/repos/n8n/node_modules/.pnpm/jest@29.7.0_@types+node@20.19.21_ts-node@10.9.2_@swc+core@1.15.7_@swc+helpers@0.5.17__@_897206e04065c0608fc74f2b315f766b/node_modules:/Users/maxim/repos/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxim/repos/n8n/node_modules/.pnpm/jest@29.7.0_@types+node@20.19.21_ts-node@10.9.2_@swc+core@1.15.7_@swc+helpers@0.5.17__@_897206e04065c0608fc74f2b315f766b/node_modules/jest/bin/node_modules:/Users/maxim/repos/n8n/node_modules/.pnpm/jest@29.7.0_@types+node@20.19.21_ts-node@10.9.2_@swc+core@1.15.7_@swc+helpers@0.5.17__@_897206e04065c0608fc74f2b315f766b/node_modules/jest/node_modules:/Users/maxim/repos/n8n/node_modules/.pnpm/jest@29.7.0_@types+node@20.19.21_ts-node@10.9.2_@swc+core@1.15.7_@swc+helpers@0.5.17__@_897206e04065c0608fc74f2b315f766b/node_modules:/Users/maxim/repos/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
