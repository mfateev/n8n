						},
						{
							"id": "a2",
							"name": "counter",
							"value": "10",
							"type": "number"
						}
					]
				},
				"includeOtherFields": true,
				"options": {}
			}
		},
		{
			"id": "set-2",
			"name": "Transform Data",
			"type": "n8n-nodes-base.set",
			"typeVersion": 3.4,
			"position": [400, 0],
			"parameters": {
				"mode": "manual",
				"duplicateItem": false,
				"assignments": {
					"assignments": [
						{
							"id": "b1",
							"name": "step",
							"value": "2",
							"type": "string"
						},
						{
							"id": "b2",
							"name": "doubled",
							"value": "={{ $json.counter * 2 }}",
							"type": "number"
						},
						{
							"id": "b3",
							"name": "previousStep",
							"value": "={{ $json.step }}",
							"type": "string"
						}
					]
				},
				"includeOtherFields": true,
				"options": {}
			}
		},
		{
			"id": "set-3",
			"name": "Final Output",
			"type": "n8n-nodes-base.set",
			"typeVersion": 3.4,
			"position": [600, 0],
			"parameters": {
				"mode": "manual",
				"duplicateItem": false,
				"assignments": {
					"assignments": [
						{
							"id": "c1",
							"name": "step",
							"value": "3",
							"type": "string"
						},
						{
							"id": "c2",
							"name": "summary",
							"value": "={{ 'Counter was ' + $json.counter + ', doubled to ' + $json.doubled }}",
							"type": "string"
						},
						{
							"id": "c3",
							"name": "allSteps",
							"value": "={{ [$json.previousStep, $json.step, '3'].join(' -> ') }}",
							"type": "string"
						}
					]
				},
				"includeOtherFields": true,
				"options": {}
			}
