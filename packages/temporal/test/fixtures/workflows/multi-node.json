{
	"name": "Multi-Node Sequential Workflow",
	"nodes": [
		{
			"id": "trigger-1",
			"name": "Manual Trigger",
			"type": "n8n-nodes-base.manualTrigger",
			"typeVersion": 1,
			"position": [0, 0],
			"parameters": {}
		},
		{
			"id": "set-1",
			"name": "Set Initial Data",
			"type": "n8n-nodes-base.set",
			"typeVersion": 3.4,
			"position": [200, 0],
			"parameters": {
				"mode": "manual",
				"duplicateItem": false,
				"assignments": {
					"assignments": [
						{
							"id": "a1",
							"name": "step",
							"value": "1",
